pipeline {
    agent { label 'master' }
    parameters {
        string(name: 'JOB_NUM', defaultValue: '', description: '')
    }
    
    stages {
        stage('Print Job Num') {
            steps {
                powershell '''
                           Write-Host 
                           "Job Number of Job1 Is: $ENV:JOB_NUM"
                           '''   
            }
        }
    }
}